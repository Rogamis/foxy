{
  "expo": {
    "name": "red-fox",
    "slug": "red-foxy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/foxy.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/foxy.png",
      "resizeMode": "contain",
      "backgroundColor": "#F45C43"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/foxy.png",
        "backgroundColor": "#F45C43"
      }
    },
    "web": {
      "favicon": "./assets/foxy.png"
    }
  }
}
